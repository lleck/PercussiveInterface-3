        <PAGEOBJECT XPOS="156.693543307087" YPOS="3415.86645669292" OwnPage="3" ItemID="2006335305" PTYPE="4" WIDTH="226.772086614173" HEIGHT="23" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" SHADE="1" SHADE2="1" PLINEART="1" isWeldItem="1" WeldSource="2006335305" ANNAME="Footnote frame Default (3)" TEXTFLOWMODE="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" LOCKR="1" path="M0 0 L226.772 0 L226.772 23 L0 23 L0 0 Z" copath="M0 0 L226.772 0 L226.772 23 L0 23 L0 0 Z" gXpos="156.693543307087" gYpos="2966.9050228795" gWidth="0" gHeight="0" PSTYLE="Fließtext" LAYER="0" NEXTITEM="-1" BACKITEM="-1" isNoteFrame="1">
